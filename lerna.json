{
  "$schema": "node_modules/lerna/schemas/lerna-schema-json",
  "useWorkspaces": true,
  "useNx": true,
  "packages": [
    "packages/*"
  ],
  "version": "0.10.0-beta.7",
  "command": {
    "bootstrap": {
      "hoist": "**"
    },
    "version": {
      "noPush": true
    },
    "publish": {
      "fromPackage": true,
      "message": "Release %s",
      "noPush": true
    }
  }
}
