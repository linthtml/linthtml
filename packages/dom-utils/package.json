    "./is_void_node": {
      "types": "./dist/is_void_node.d.ts",
      "import": "./dist/is_void_node.js",
      "require": "./dist/is_void_node.cjs"
    },
    "./tags": {
      "types": "./dist/tags.d.ts",
      "import": "./dist/tags.js",
      "require": "./dist/tags.cjs"
    },
    "./text_node": {
      "types": "./dist/text_node.d.ts",
